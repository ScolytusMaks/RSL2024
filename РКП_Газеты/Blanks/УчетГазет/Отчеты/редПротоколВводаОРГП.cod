-- 2_Обработчики событий шаблона0. --

  proc шаблон_ПриСоздании(Context :Variant);
    inherited шаблон_ПриСоздании(Context);
  end;

  proc шаблон_ПриОткрытии(Create :Logical);
    var locCell :TemplateCell;
    inherited шаблон_ПриОткрытии(Create);
    locCell = фреймКнопки2.CellByField['ИзмененныеЗаписи'];
    if locCell <> nil then
      if (Record.ClassInfo as RecordClassInfo).MemberByName('ИзмененныеЗаписи') = nil then
        locCell.Owner.Row[locCell.Row].Visible = false;
      else
        locCell.Owner.ParentFrame.Visible = true;
        locCell.Owner.Visible = true;
        locCell.Owner.Row[locCell.Row].Visible = true;
      fi;
    fi;
  end;

  proc шаблон_ПриСчитывании;
    inherited шаблон_ПриСчитывании;
    МодифицироватьШаблон;
  end;

-- 2_Обработчики событий клеток шаблона0. --

  func Поле_ПриВыводе_ЗаголовокОкнаФормы(Cell :TemplateCell; Value :Variant; Action :Template.OutputTypes; var Format :String) :Variant;
    Result = 'Протокол ввода данных ОРГП' + Br +
             '№ ' + Str(НомерОтчета) + ' / ' + Str(ЗаГод) + Br +
             '<s-><s->за период с ' + Str(ДатаН) + ' по  ' + Str(ДатаК) + ' включительно';
  end;

  func Поле_ПриНажатии_Уточнение(Cell :TemplateCell; Action :Template.ClickTypes) :Logical;
    var локФактДатаК :Date;
    var локОператорИмя, локОператор :String;
    if Dat(Day(UpdateDate), Mon(UpdateDate), Day(UpdateDate)) = Dat(Day(ДатаК), Mon(ДатаК), Day(ДатаК)) then
      локФактДатаК = AddSec(UpdateDate, 10);
    else
      локФактДатаК = ДатаК + 1;
    fi;
    if (Cell.Owner = секцГазеты) then
      локОператорИмя = Газеты[Cell.Frame].ОператорИмя;
      локОператор    = Газеты[Cell.Frame].Оператор;
      if (Cell.Contents = 'Введено') then          УчетГазет.Отчеты.блПротоколВвода_Детализация.ПоГазетам( 'ОРГП', 'Газеты', локОператорИмя, локОператор, ДатаН, локФактДатаК, wsRecCreated );
      elsif (Cell.Contents = 'Редактировано') then УчетГазет.Отчеты.блПротоколВвода_Детализация.ПоГазетам( 'ОРГП', 'Газеты', локОператорИмя, локОператор, ДатаН, локФактДатаК, wsRecUpdated );
      fi;
    elsif (Cell.Owner = секцНомера) then
      локОператорИмя = Номера[Cell.Frame].ОператорИмя;
      локОператор    = Номера[Cell.Frame].Оператор;
      if (Cell.Contents = 'Введено') then          УчетГазет.Отчеты.блПротоколВвода_Детализация.ПоГазетам( 'ОРГП', 'Номера', локОператорИмя, локОператор, ДатаН, локФактДатаК, wsRecCreated );
      elsif (Cell.Contents = 'Редактировано') then УчетГазет.Отчеты.блПротоколВвода_Детализация.ПоГазетам( 'ОРГП', 'Номера', локОператорИмя, локОператор, ДатаН, локФактДатаК, wsRecUpdated );
      fi;
    fi;
  end;

  proc Поле_ПриВыходе_Комментарий(Cell :TemplateCell; Index :Integer);
    var locUpdateDate : Date;
    locUpdateDate = Record.UpdateDate;
    РКП_СИС.Recs.RecordPostIfNeed(Record);
    Record.UpdateDate = locUpdateDate;
  end;

  func Поле_ПриНажатии_ИзмененныеЗаписи(Cell :TemplateCell; Action :Template.ClickTypes) :Logical;
    ИзмененныеЗаписи = not ИзмененныеЗаписи;
    МодифицироватьШаблон;
  end;


