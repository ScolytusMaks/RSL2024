record ЕдиницаХранения inherited СИС2.Базовая.БазоваяЗапись title "Единица хранения";
  field НомерГазеты     :РКП_Газеты.НомерГазеты     title "Номер газеты";
  field УчетныйДокумент :РКП_Газеты.УчетныйДокумент title "Учетный документ";
  field НомерПозиции    :Integer                    title "Номер позиции";
  field ДатаПоступления :Date                       title "Дата поступления экземпляров";
  field Количество      :Integer                    title "Количество";
  field Комментарий     :String                     title "Комментарий";
  field ДатаПодшивки    :Date                       title "Дата включения в учетный документ";
end;