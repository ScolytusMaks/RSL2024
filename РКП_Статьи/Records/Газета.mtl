Extends РКП_Газеты.Газета ;
  field НазваниеЛГС :String(MaxIndexedStr) title "Название"; -- Условное название издания для ЛГС
  field ПриложениеКГазете :РКП_Газеты.Газета title "Приложение"; -- Приложение к газете. В том случае, если ссылка непустая, по-другому формируется биб.запись
  field КодИздания :String title "Код издания"; -- Условный код издания для ЛГС
  field РосписьСтатей :logical title "Использовать в системе 'Роспись статей'";
end;