class inherited Статьи.редВыпускЛГС_07 "Создание оригинал-макета", editor ВыпускЛГС;
Import РКП_СИС Classes StdCode;

inclass

inobject

-----------------------------------------------------------------------------------------------------------
--{ Обработчики событий бланка
  Proc СозданиеОригиналМакета;
    var ЛГС                     : TextFile;
----Создание файла-----------------------------------------------------------------------------------------
      ОригиналМакет.ЛГС_01_Файл.Создание(ПапкаЛетописи,ФайлЛетописи,ЛГС);
----Описание параметров всего документа--------------------------------------------------------------------
      ОригиналМакет.ЛГС_02_ПарамДок.Создание(ЛГС,Record);
----ПОЕХАЛИ!!!---------------------------------------------------------------------------------------------
      ОригиналМакет.ЛГС_03_ТитЛист.Создание(ЛГС,Record);
-----------------------------------------------------------------------------------------------------------
      ОригиналМакет.ЛГС_04_Содержание.Создание(ЛГС,Record);
----Создание предисловия для выпуска N 1 ------------------------------------------------------------------
    if ВыпускЛетописи  = 1      then
      ОригиналМакет.ЛГС_05_Предисловие.Создание(ЛГС);
    end;
----Создание списка газет для выпуска N 1 -----------------------------------------------------------------
    if ВыпускЛетописи  = 1      then
      ОригиналМакет.ЛГС_06_СписокГазетГода.Создание(ЛГС,Record);
    end;
----Создание основного текста------------------------------------------------------------------------------
      ОригиналМакет.ЛГС_07_ОсновнойТекст.Создание(ЛГС,Record);
----Составление именного указателя-------------------------------------------------------------------------
      ОригиналМакет.ЛГС_08_ИменнойУказатель.Создание(ЛГС,Record);
----Составление географического указателя------------------------------------------------------------------
      ОригиналМакет.ЛГС_09_ГеографическийУказатель.Создание(ЛГС,Record);
----Список газет, материалы из которых...------------------------------------------------------------------
      ОригиналМакет.ЛГС_10_СписокГазетВыпуска.Создание(ЛГС,Record);
----Нумерационный указатель библиографиеских записей.....карточки------------------------------------------
      ОригиналМакет.ЛГС_11_УказательКарточек.Создание(ЛГС,Record);
----Перечень рубрик...для первого номера ЛГС---------------------------------------------------------------
    if ВыпускЛетописи=1                                 then
      ОригиналМакет.ЛГС_12_ПереченьРубрикУДК.Создание(ЛГС,Record);
    end;
----Перечень временных тематических рубрик-----------------------------------------------------------------
    if ВыпускЛетописи=52                                then
      ОригиналМакет.ЛГС_13_УказательВременныхРубрик.Создание(ЛГС,Record);
    end;
----Сотавители ЛГС-----------------------------------------------------------------------------------------
      ОригиналМакет.ЛГС_14_Составители.Создание(ЛГС,Record);
----конец RTF----------------------------------------------------------------------------------------------
                ЛГС.WriteLn("}");                                      --Конец секции
                ЛГС.WriteLn("}");
  end; --СозданиеОригиналМакета
--}
end