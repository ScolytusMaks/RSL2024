class "";

inclass

--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  proc Создание(ЛГС             : TextFile;
                ЗаписьЛГС       : Record);
                ЛГС.WriteLn("");
                if  ЗаписьЛГС.ВыпускЛетописи=1                  then
                ЛГС.WriteLn("\par\page\sect\sbknone\cols0");
                else
                ЛГС.WriteLn("\par\page\sect\sbknone\cols0\par\ql");
                end;
                ЛГС.WriteLn("\line\line\line\line\line\line\line\line");
                ЛГС.WriteLn("\          Ответственный редактор\line");
                ЛГС.WriteLn("\          \b "+ЗаписьЛГС.ОтвРедактор+"\b0\line");
                ЛГС.WriteLn("\line");
                ЛГС.WriteLn("\          Редактор-составитель\line");
                ЛГС.WriteLn("\          \b "+ЗаписьЛГС.РедСоставитель+"\b0\line");
                ЛГС.WriteLn("\line");
                ЛГС.WriteLn("\          Ответственный за выпуск\line");
                ЛГС.WriteLn("\          \b "+ЗаписьЛГС.ОтвЗаВыпуск+"\b0\line");
                ЛГС.WriteLn("\line");
                ЛГС.WriteLn("\          Художественное оформление\line");
                ЛГС.WriteLn("\          \b "+ЗаписьЛГС.ХудОформление+"\b0\line");
                ЛГС.WriteLn("\line");
                ЛГС.WriteLn("\          Компьютерная верстка\line");
                ЛГС.WriteLn("\          \b "+ЗаписьЛГС.КомпВерстка+"\b0\line");
                ЛГС.WriteLn("\line");
                ЛГС.WriteLn("\line");
                ЛГС.WriteLn("\          Изд. лиц. № 06028 от 12.10.2001.\line");
                ЛГС.WriteLn("\          Подписано в печать "+Str(ЗаписьЛГС.ПодписаноВПечать)+".\line");
                ЛГС.WriteLn("\          Формат 60x88/16. Бумага офсетная № 2.\line");
                ЛГС.WriteLn("\          Ризограф\line");
                ЛГС.WriteLn("\          Усл.-печ.л."+Str(ЗаписьЛГС.УслПечЛ,2)+". Уч.-изд.л."+Str(ЗаписьЛГС.УчИздЛ,2)+".\line");
                ЛГС.WriteLn("\          Тираж "+Str(ЗаписьЛГС.ТиражВыпуска)+" экз. Заказ\line");
                ЛГС.WriteLn("\line");
                ЛГС.WriteLn('\          Издательство ООО "Бук Чембэр Интернэшнл"\line');
                ЛГС.WriteLn("\          119034, Москва, ул. Остоженка, 4\line");
                ЛГС.WriteLn("\          тел. 291-12-78, 291-96-30\line");
  end;
--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

end