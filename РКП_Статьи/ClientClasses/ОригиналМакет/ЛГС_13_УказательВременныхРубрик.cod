class "";

inclass

--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  proc Создание(ЛГС             : TextFile;
                ЗаписьЛГС       : Record);
    var j                       : integer;
                ЛГС.WriteLn("\par\page\sect\sbknone\cols0");
                ЛГС.WriteLn("\par\pard"+ОригиналМакет.Стиль.styl2+"\qc ");
                ЛГС.WriteLn("{\b Указатель временных тематических рубрик\b}");
                ЛГС.WriteLn("\line");
                ЛГС.WriteLn('{\b в "Летописи газетных статей" №1 - 52 за '+Str(ЗаписьЛГС.ГодЛетописи)+'год \b}');
                ЛГС.WriteLn("\line");
      for j = 1..ЗаписьЛГС.УказательВТР.Count                           do
                ЛГС.WriteLn("\par"+
                            ОригиналМакет.Стиль.styl1+
                            "\ql "+
                            "{"+ЗаписьЛГС.УказательВТР.Items[j].Рубрика.Наим+" "+
                            ЗаписьЛГС.УказательВТР.Items[j].НомераЛГС+
                            "}"+
                            "\ds1");
      end;
                ЛГС.WriteLn("\ds2");
                ЛГС.WriteLn("");
  end;
--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~


end