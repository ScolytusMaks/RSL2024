class "";

inclass
--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  proc Создание(ПапкаКарточек:string;ФайлКарточек:string;var ПК:TextFile);
    if    ExistFolder(ПапкаКарточек) = false    then
      CreateFolder(ПапкаКарточек);
    end;
    if  ExistFile(ФайлКарточек) = true          then
      RemoveFile(ФайлКарточек);
    end;
    ПК          = TextFile.Create(ФайлКарточек,fmCreate);
  end;
--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~


end