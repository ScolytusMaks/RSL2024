class "";

inclass

--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
  proc Создание(ЛГС             : TextFile;
                ЗаписьЛГС       : Record);
    var j                       : integer;
                ЛГС.WriteLn("\sect\sbknone\cols0");                             --Начало секции об обном столбце с новой страницы
                ЛГС.WriteLn("\par\pard"+ОригиналМакет.Стиль.styl2+"\qc ");
                ЛГС.WriteLn("{\b Список газет, материалы из которых отражены в № "+
                            Str(ЗаписьЛГС.ВыпускЛетописи)+
                            "\b0}");
                ЛГС.WriteLn("\line ");
                ЛГС.WriteLn('{\b "Летописи газетных статей" \b0}');
                ЛГС.WriteLn("\ds2");
      for j = 1..ЗаписьЛГС.УказательГазет.Count         do
                ЛГС.WriteLn("\par"+
                            ОригиналМакет.Стиль.styl1+
                            "\ql "+
                            "{"+
                            --РКП_Статьи.ОригиналМакет.ОбработкаТекста.ЗаменаТире(ЗаписьЛГС.УказательГазет.Items[j].БибОписание)+
                            РКП_Статьи.ОригиналМакет.ОбработкаТекста.ВставкаСпецСимволов(ЗаписьЛГС.УказательГазет.Items[j].БибОписание)+
                            --ЗаписьЛГС.УказательГазет.Items[j].БибОписание+
                            РКП_Статьи.ОригиналМакет.ОбработкаТекста.ВставкаСпецСимволов(ЗаписьЛГС.УказательГазет.Items[j].Номера)+
                            ". }"+
                            "\ds1");
      end;
    end;
--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

end