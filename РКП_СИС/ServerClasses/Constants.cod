Class "Константы, используемые проектом";

inclass public

--== И Н Ф О Р М А Ц И Я   О   П Р О Е К Т Е ==--

  ИмяВерсииПроекта: String = Self.ClassProject + '.ВерсияПроекта';

  func ПолучитьВерсиюПроекта: Numeric;
    var MI: MethodInfo;
    MI = СИС2.Функции.ClassInfo.MemberByName('СформироватьВерсиюПроекта') as MethodInfo;
    Return MI.CallFunc(MI, Self.ClassProject); -- обход ограничения компиляции для клиентских классов
  end;

  ЗаголовокИнформацииОРелизах :String = 'Системные проекты';
  ЗаголовокИнформацииОРелизахРГБ :String = 'Российская государственная библиотека';
  ГодВыпускаПервойГазеты :Integer = 1703; -- год выпуска первой газеты в России

end